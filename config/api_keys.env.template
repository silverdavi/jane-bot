# API Keys for Voice Assistant
# Copy this file to api_keys.env and fill in your actual API keys

# OpenAI API Key (required for transcription and TTS)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key (required for KB management and embeddings)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Porcupine Access Key (for custom wake words)
PORCUPINE_ACCESS_KEY=your_porcupine_access_key_here

# Optional: Google Cloud Storage (for future cloud migration)
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json 